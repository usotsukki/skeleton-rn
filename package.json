{
	"name": "skeleton",
	"version": "1.0.0",
	"private": true,
	"main": "expo-router/entry",
	"packageManager": "yarn@1.22.1",
	"expo": {
		"doctor": {
			"reactNativeDirectoryCheck": {
				"listUnknownPackages": false
			}
		}
	},
	"scripts": {
		"android": "expo run:android",
		"android:prebuild": "expo prebuild -p android",
		"android:rebuild": "run-s android:prebuild android",
		"eas-android": "dotenv -e .env -- eas build --no-wait -e development -p android",
		"eas-android:prod": "dotenv -e .env -- eas build --no-wait -e production -p android",
		"eas-android:prod-apk": "dotenv -e .env -- eas build --no-wait -e prod-apk -p android",
		"eas-ios": "dotenv -e .env -- eas build --no-wait -e development -p ios",
		"eas-deploy:preview": "dotenv -e .env -- eas deploy",
		"eas-deploy:prod": "dotenv -e .env -- eas deploy --prod",
		"eas-run": "dotenv -e .env -- eas build:run --latest -e development -p",
		"export-server": "expo export -p web && rm -rf ./dist/client",
		"check-unused-deps": "./scripts/check-unused-deps.sh",
		"duplication:check": "jscpd --config jscpd.json",
		"duplication:view": "jscpd --config jscpd.json && open ./cpd/html/index.html",
		"increment-version": "./scripts/bump-version.sh",
		"install-pods": "(cd ios && pod install)",
		"ios": "expo run:ios --no-bundler",
		"ios:prebuild": "expo prebuild -p ios",
		"ios:prebuild:clean": "expo prebuild -p ios --clean",
		"ios:rebuild": "run-s ios:prebuild:clean ios",
		"lint": "run-p lint:*",
		"lint-commit": "commitlint --config commitlint.config.js --edit ",
		"lint:format": "prettier --write .",
		"lint:format:check": "prettier --check .",
		"lint:js": "eslint --cache . --quiet",
		"lint:ts": "tsc --skipLibCheck --noEmit",
		"nuke": "./scripts/nuke.sh",
		"postinstall": "patch-package",
		"prepare": "husky",
		"start": "expo start --dev-client",
		"switch-env": "./scripts/switch-env.sh",
		"test": "jest src",
		"test-e2e": "./scripts/run-maestro-tests.sh",
		"test-detox:build": "detox build -c ios.sim.debug",
		"test-detox": "detox test -c ios.sim.debug"
	},
	"dependencies": {
		"@babel/runtime": "^7.25.6",
