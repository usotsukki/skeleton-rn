{
	"expo": {
		"name": "Skeleton",
		"slug": "skeleton",
		"version": "1.3.0",
		"icon": "./assets/png/app-icon.png",
		"scheme": "skeleton",
		"newArchEnabled": true,
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		},
		"splash": {
			"image": "./assets/png/image-splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#151B26"
		},
		"ios": {
			"buildNumber": "1.3.0",
			"appleTeamId": "2ZDPCXEHU3"
		},
		"android": {
			"versionCode": 53010300
		},
		"web": {
			"bundler": "metro",
			"output": "server"
		},
		"plugins": [
			"@react-native-google-signin/google-signin",
			"@react-native-firebase/app",
			"@react-native-firebase/auth",
			"expo-apple-authentication",
			"expo-router",
			"expo-localization",
			[
				"expo-font",
				{
					"fonts": [
						"./assets/fonts/SF-Pro-Rounded-Black.otf",
						"./assets/fonts/SF-Pro-Rounded-Bold.otf",
						"./assets/fonts/SF-Pro-Rounded-Heavy.otf",
						"./assets/fonts/SF-Pro-Rounded-Medium.otf",
						"./assets/fonts/SF-Pro-Rounded-Regular.otf",
						"./assets/fonts/SF-Pro-Rounded-Semibold.otf",
						"./assets/fonts/SF-Mono-Bold.otf",
						"./assets/fonts/SF-Mono-Semibold.otf"
					],
					"android": {
						"fonts": [
							{
								"fontFamily": "SF Pro Rounded",
								"fontDefinitions": [
									{
										"path": "./assets/fonts/SF-Pro-Rounded-Black.otf",
										"weight": "900"
									},
									{
										"path": "./assets/fonts/SF-Pro-Rounded-Heavy.otf",
										"weight": "800"
									},
									{
										"path": "./assets/fonts/SF-Pro-Rounded-Bold.otf",
										"weight": "700"
									},
									{
										"path": "./assets/fonts/SF-Pro-Rounded-Semibold.otf",
										"weight": "600"
									},
									{
										"path": "./assets/fonts/SF-Pro-Rounded-Medium.otf",
										"weight": "500"
									},
									{
										"path": "./assets/fonts/SF-Pro-Rounded-Regular.otf",
										"weight": "400"
									}
								]
							},
							{
								"fontFamily": "SF Mono",
								"fontDefinitions": [
									{
										"path": "./assets/fonts/SF-Mono-Bold.otf",
										"weight": "700"
									},
									{
										"path": "./assets/fonts/SF-Mono-Semibold.otf",
										"weight": "600"
									}
								]
							}
						]
					}
				}
			],
			[
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static",
						"forceStaticLinking": ["RNFBApp", "RNFBAuth"],
						"deploymentTarget": "16.0.0"
					}
				}
			],
			[
				"expo-notifications",
				{
					"icon": "./assets/png/logo.png",
					"color": "#ffffff",
					"defaultChannel": "default"
				}
			],
			[
				"expo-dev-client",
				{
					"launchMode": "most-recent",
					"addGeneratedScheme": false
				}
			],
			[
				"expo-screen-orientation",
				{
					"initialOrientation": "DEFAULT"
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you use the OCR feature."
				}
			],
			[
				"expo-camera",
				{
					"cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
					"microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
					"recordAudioAndroid": true
				}
			]
		]
	}
}
